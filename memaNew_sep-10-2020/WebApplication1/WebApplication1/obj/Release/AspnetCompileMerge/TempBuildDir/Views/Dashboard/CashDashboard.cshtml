                            <option value="3" >ASK</option>
                            <option value="4" >SCB</option>
                            <option value="5" >HBL</option>
                        </select>*@

                    <input type="submit" value="Submit">
                }
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>

