@model IEnumerable<WebApplication1.Models.Bank_Details>
<table class="table table-bordered" id="myTable">
    <thead>
        <tr>

            <th>Bank_ID</th>
            <th>Bank</th>
            <th>Org</th>
            <th>Branch</th>
            <th>Account</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.Bank_ID</td>
                <td>@item.Bank</td>
                <td>@item.Org</td>
                <td>@item.Branch</td>
                <td>@item.Account</td>

                <td>
                    <div class="btn-group">
                        <a href="#" class="btn btn-primary BtnEdit" id="@item.Bank_ID">Update/View</a>
                        @*<a href="@Url.Action("BankPaymentVoucherSubmitByID", "Edit", new { id = item.Bank_ID })" Class="btn btn-primary" pendingRecordId="@item.Bank_ID">Submit</a>*@

                    </div>
                </td>
            </tr>
        }



    </tbody>
</table>


