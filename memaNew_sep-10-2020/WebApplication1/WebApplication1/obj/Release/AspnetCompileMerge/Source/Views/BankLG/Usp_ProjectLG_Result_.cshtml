@*@model IEnumerable<WebApplication1.Models.Usp_APLG_Result>*@


<table class="table table-bordered" id="myTable">
    <thead>
        <tr>
            <th>S.no</th>
            <th>Date</th>
            <th>Project</th>
            <th>Description</th>
            <th>Vendor</th>
            <th>Voucher_Type</th>
            <th>Voucher_No</th>
            <th>Debit</th>
            <th>Credit</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
        @*@foreach (var item in ViewBag.BankGlList)*@
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.id</td>
                <td>@String.Format("{0:dd/MM/yyyy}", item.Date)</td>
                <td>@item.Project</td>
                <td>@item.Description</td>
                <td>@item.Vendor</td>
                <td>@item.Voucher_Type</td>
                <td>@item.Voucher_No</td>               
                <td>@String.Format("{0:#,0}", (item.Debit == null ? 0 : item.Debit))</td>
                <td>@String.Format("{0:#,0}", (item.Credit == null ? 0 : item.Credit))</td>
                <td>@String.Format("{0:#,0}", (item.Balance__ == null ? 0 : item.Balance__))</td>
                @*<td>
                        <div class="btn-group">
                            <a href="#" class="btn btn-primary BtnEdit" id="@item.Bank_ID">Update/View</a>
                            <a href="@Url.Action("BankPaymentVoucherSubmitByID", "Edit", new { id = item.Bank_ID })" Class="btn btn-primary" pendingRecordId="@item.Bank_ID">Submit</a>

                        </div>
                    </td>*@
            </tr>
        }



    </tbody>
</table>




