@model IEnumerable<WebApplication1.Models.Control_Details>

<table class="table table-bordered" id="myTable">
    <thead>
        <tr>

            <th>Control_ID</th>
            <th>Control</th>>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.Control_ID</td>
                <td>@item.Control</td>


                <td>
                    <div class="btn-group">
                        <a href="#" class="btn btn-primary BtnEdit" id="@item.Control_ID">Update/View</a>
                        @*<a href="@Url.Action("BankPaymentVoucherSubmitByID", "Edit", new { id = item.Bank_ID })" Class="btn btn-primary" pendingRecordId="@item.Bank_ID">Submit</a>*@

                    </div>
                </td>
            </tr>
        }



    </tbody>
</table>



