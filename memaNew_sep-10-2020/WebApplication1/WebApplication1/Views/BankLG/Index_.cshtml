@model IEnumerable<WebApplication1.Models.Usp_BankGL_Result>
<table class="table table-bordered" id="myTable">
    <thead>
        <tr>
            <th>S.no</th>
            <th>Date</th>
            <th>Bank</th>
            <th>Description</th>
            <th>Vendor</th>
            <th>Project</th>
            <th>Cheque_No</th>
            <th>Debit</th>
            <th>Credit</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
       
            @foreach (var item in Model)
            {
               
            <tr>
                <td>@item.id</td>
                <td>@String.Format("{0:dd/MM/yyyy}", @item.Date)</td>
                <td>@item.Bank</td>
                <td>@item.Description</td>
                <td>@item.Vendor</td>
                <td>@item.Project</td>
                <td>@item.Cheque_No</td>
                <td>@String.Format("{0:#,0}", (item.Debit == null ? 0 : item.Debit))</td>
                <td>@String.Format("{0:#,0}", (item.Credit == null ? 0 : item.Credit))</td>
                <td>@String.Format("{0:#,0}", (item.Balance == null ? 0 : item.Balance))</td>

            </tr>
        }



    </tbody>
</table>


